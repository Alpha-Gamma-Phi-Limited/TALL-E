{
  "manifest_version": 3,
  "name": "WorthIt Assistant",
  "description": "Compare prices and value insights while shopping online.",
  "version": "0.1.0",
  "action": {
    "default_title": "WorthIt",
    "default_popup": "popup.html"
  },
  "permissions": ["storage", "tabs"],
  "host_permissions": ["http://localhost:8000/*"]
}
